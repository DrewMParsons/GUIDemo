<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="800.0" stylesheets="@../stylesheets/graystyle.css" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="guidemo.FXMLDocumentController">
   <children>
      <HBox layoutX="8.0" layoutY="300.0" prefHeight="300.0" prefWidth="800.0" spacing="20.0">
         <children>
            <VBox prefHeight="300.0" prefWidth="200.0">
               <children>
                  <Label alignment="TOP_LEFT" text="ListView Object">
                     <font>
                        <Font name="System Bold Italic" size="18.0" />
                     </font>
                  </Label>
                  <ListView fx:id="listView" prefHeight="300.0" prefWidth="200.0" />
               </children>
            </VBox>
            <StackPane prefHeight="300.0" prefWidth="200.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#listViewButtonPushed" text="Selected Gear" />
               </children>
            </StackPane>
            <VBox prefHeight="300.0" prefWidth="200.0">
               <children>
                  <Label text="TextArea Object">
                     <font>
                        <Font name="System Bold Italic" size="18.0" />
                     </font>
                  </Label>
                  <TextArea fx:id="golfTextArea" prefHeight="300.0" prefWidth="200.0" />
               </children>
            </VBox>
            <AnchorPane prefHeight="300.0" prefWidth="200.0">
               <children>
                  <Button layoutX="75.0" layoutY="241.0" mnemonicParsing="false" onAction="#changeScreenButtonPushed" text="Go To TableView" />
               </children>
            </AnchorPane>
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </HBox>
      <HBox prefHeight="300.0" prefWidth="800.0" spacing="10.0">
         <children>
            <VBox prefHeight="300.0" prefWidth="200.0" spacing="10.0">
               <children>
                  <Label text=" Check Box Objects">
                     <font>
                        <Font name="System Bold Italic" size="18.0" />
                     </font>
                  </Label>
                  <CheckBox fx:id="pepperoniCheckBox" mnemonicParsing="false" text="Peppperoni" />
                  <CheckBox fx:id="pineappleCheckBox" mnemonicParsing="false" text="Pineapple" />
                  <CheckBox fx:id="baconCheckBox" mnemonicParsing="false" text="Bacon" />
                  <Button mnemonicParsing="false" onAction="#pizzaOrderButtonPushed" styleClass="root" text="Order Pizza " />
                  <Label fx:id="pizzaOrderLabel" alignment="TOP_LEFT" prefHeight="105.0" prefWidth="148.0" text="label" />
               </children>
            </VBox>
            <VBox prefHeight="300.0" prefWidth="200.0" spacing="10.0">
               <children>
                  <Label fx:id="label" alignment="TOP_LEFT" prefHeight="36.0" prefWidth="199.0" text="ChoiceBox Object">
                     <font>
                        <Font name="System Bold Italic" size="18.0" />
                     </font>
                  </Label>
                  <ChoiceBox fx:id="choiceBox" prefWidth="150.0" />
                  <Button mnemonicParsing="false" onAction="#choiceBoxButtonPushed" text="Favorite Fruit" />
                  <Label fx:id="choiceBoxLabel" text="Label" />
               </children>
            </VBox>
            <VBox prefHeight="300.0" prefWidth="200.0" spacing="10.0">
               <children>
                  <Label text="Combo Box Options">
                     <font>
                        <Font name="System Bold Italic" size="18.0" />
                     </font>
                  </Label>
                  <ComboBox fx:id="comboBox" onAction="#comboBoxWasUpdated" prefWidth="150.0" />
                  <Label fx:id="comboBoxLabel" text="Label" />
               </children>
            </VBox>
            <VBox prefHeight="300.0" prefWidth="200.0" spacing="10.0">
               <children>
                  <Label text="Radio Button Options">
                     <font>
                        <Font name="System Bold Italic" size="18.0" />
                     </font>
                  </Label>
                  <RadioButton fx:id="phpRadioButton" mnemonicParsing="false" onAction="#radioButtonChanged" text="PHP" />
                  <RadioButton fx:id="javaRadioButton" mnemonicParsing="false" onAction="#radioButtonChanged" text="Java" />
                  <RadioButton fx:id="cSharpRadioButton" mnemonicParsing="false" onAction="#radioButtonChanged" text="C#" />
                  <RadioButton fx:id="cPlusPlusRadioButton" mnemonicParsing="false" onAction="#radioButtonChanged" text="C++" />
                  <Label fx:id="radioButtonLabel" text="Label" />
               </children>
            </VBox>
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </HBox>
   </children>
</AnchorPane>
